{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": 2
  },
  "extends": "airbnb",
  "plugins": ["react"],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  }
}
